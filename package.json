  "lint-staged": {
    "{cypress,src}/**/*.{js,jsx}": [
      "prettier --write --trailing-comma es5",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/*.test.(js|jsx)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@sendgrid)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "<rootDir>/SCSSStub.js"
    }
  }
