{
  "name": "sgbouncewizard",
  "version": "1.0.0",
  "description": "UCI Capstone 2018-19 bounce wizard",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "cypress:open:localhost": "cypress open",
    "cypress:open:mock": "cypress open --env API_URL=http://localhost:3004",
    "cypress:open:app": "cypress open --config baseUrl=http://deployedappurl.cloudfront.net",
    "cypress:run:localhost": "cypress run",
    "cypress:run:app": "cypress run --config baseUrl=http://deployedappurl.cloudfront.net",
    "dev": "webpack-dev-server --env.ENVIRONMENT=dev --open --mode=development ",
    "dev:mock": "webpack-dev-server --env.ENVIRONMENT=mock --open --mode=development ",
    "precommit": "lint-staged",
    "prepush": "jest",
    "storybook": "start-storybook -p 6009",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "json-server": "json-server --watch db.json -p 3004"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/productdana/sgbouncewizard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/productdana/sgbouncewizard/issues"
  },
  "homepage": "https://github.com/productdana/sgbouncewizard#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.3",
    "@storybook/addon-links": "^4.0.0-alpha.3",
    "@storybook/addons": "^4.0.0-alpha.3",
    "@storybook/react": "^4.0.0-alpha.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "cypress": "^3.1.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "json-server": "^0.14.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "storybook-addon-jsx": "^5.4.0",
    "storybook-react-router": "^1.0.2",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@sendgrid/ui-components": "^1.9.2",
    "@types/jquery": "^3.3.22",
    "@types/react": "^16.4.18",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "dotenv-webpack": "^1.5.7",
    "json-server": "^0.14.0",
    "moment": "^2.23.0",
    "react": "^16.5.2",
    "react-animate-height": "^2.0.6",
    "react-csv": "^1.0.19",
    "react-dom": "^16.5.2",
    "react-moment": "^0.8.4",
    "react-select": "^2.1.0",
    "react-test-renderer": "^16.6.0"
  },
  "lint-staged": {
    "{cypress,src}/**/*.{js,jsx}": [
      "prettier --write --trailing-comma es5",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/*.test.(js|jsx)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@sendgrid)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "<rootDir>/SCSSStub.js"
    }
  }
}
